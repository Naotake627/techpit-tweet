<!DOCTYPE html>
<html>
  <head>
    <title>TechpitTweet2</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'partial/navbar' %>

    <% if flash[:notice] %>
      <div class="alert alert-info">
        <%= flash[:notice] %>
      </div>
    <% end %>
    <% if flash[:alert] %>
      <div class="alert alert-danger">
        <%= flash[:alert] %>
      </div>
    <% end %>
<div class="logout">
    <% if user_signed_in?%>
    <%= link_to 'ログアウト',
  destroy_user_session_path,
  method: :delete
%>
<% end %>
</div>
<style>
.logout{
    text-align: right;
    padding-right:90px;
    margin-top:10px;
}
</style>

    <div class="container">
      <%= yield %>
    </div>

  </body>
</html>
